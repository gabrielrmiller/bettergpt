{
  "buildCommand": "npm run build",
  "outputDirectory": "./client/dist",
  "devCommand": "npm run dev",
  "installCommand": "cd client && npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

