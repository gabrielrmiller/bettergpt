{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!assets|.*\\.(js|css|png|svg|jpg|jpeg|gif|ico|woff|woff2|ttf|eot)).*)",
      "destination": "/index.html"
    }
  ]
}

